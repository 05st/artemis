data List<a> = Elem(a, List<a>) | Empty;

let left = readFloat(input(()));
let oper = match input(()) with
    Elem('+', Empty) -> addFloat,
    Elem('-', Empty) -> subFloat,
    Elem('*', Empty) -> mulFloat,
    Elem('/', Empty) -> divFloat,
    other -> error("Not a valid operator");
let right = readFloat(input(()));

print(showFloat(oper(left, right)));
