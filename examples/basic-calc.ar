print(concat("abc", "xyz"));

let left = readFloat(input(()));
let oper = match input(()) with
    Cons('+', Empty) -> addFloat,
    Cons('-', Empty) -> subFloat,
    Cons('*', Empty) -> mulFloat,
    Cons('/', Empty) -> divFloat,
    other -> error("Not a valid operator");
